namespace org.astria.election
import org.astria.candidate.Candidate
import org.astria.participant.AstriaAdmin

asset Election identified by electionId {
  o String electionId
  o String electionName
  o DateTime startDate
  o DateTime endDate
  --> Candidate[] candidates
  --> AstriaAdmin admin
}

transaction createElection {
  -->  AstriaAdmin admin
  o String electionName
  o DateTime startDate
  o DateTime endDate
}